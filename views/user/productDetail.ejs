  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  <script>
    const container = document.getElementById("zoom");
    const img = document.getElementById("one1");
    const container1 = document.getElementById("zoom1");
    const img1 = document.getElementById("one2");
    const container2 = document.getElementById("zoom2");
    const img2 = document.getElementById("one3");
    const container3 = document.getElementById("zoom3");
    const img3 = document.getElementById("one4");

    container.addEventListener('mousemove', (e) => {
      const rect = container.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      img.style.transformOrigin = `${x}px ${y}px`;
      img.style.transform = "scale(2)";
    });

    container.addEventListener('mouseleave', () => {
      img.style.transformOrigin = "center center";
      img.style.transform = "scale(1)";
    });
    container1.addEventListener('mousemove', (e) => {
      const rect = container1.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      img1.style.transformOrigin = `${x}px ${y}px`;
      img1.style.transform = "scale(2)";
    });

    container1.addEventListener('mouseleave', () => {
      img1.style.transformOrigin = "center center";
      img1.style.transform = "scale(1)";
    });
    container2.addEventListener('mousemove', (e) => {
      const rect = container2.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      img2.style.transformOrigin = `${x}px ${y}px`;
      img2.style.transform = "scale(2)";
    });

    container2.addEventListener('mouseleave', () => {
      img2.style.transformOrigin = "center center";
      img2.style.transform = "scale(1)";
    });
    container3.addEventListener('mousemove', (e) => {
      const rect = container3.getBoundingClientRect();
      const x = e.clientX - rect.left;
      const y = e.clientY - rect.top;
      img3.style.transformOrigin = `${x}px ${y}px`;
      img3.style.transform = "scale(2)";
    });

    container3.addEventListener('mouseleave', () => {
      img3.style.transformOrigin = "center center";
      img3.style.transform = "scale(1)";
    });

    async function addToCart(prodId) {
      
      try {
        const response = await fetch("/addToCart", {
          method: "POST",
          headers: {
            "Content-Type": "application/json"
          },
          body: JSON.stringify({ prodId })
        });
        const data = await response.json();
        
        const Toast = Swal.mixin({
          toast: true,
          position: "top-end",
          showConfirmButton: false,
          timer: 1000,
          timerProgressBar: true,
          didOpen: (toast) => {
            toast.onmouseenter = Swal.stopTimer;
            toast.onmouseleave = Swal.resumeTimer;
          }
        });

        if (data.success) {
          await Toast.fire({
            toast:true,
            position:'top-end',
            icon:'success',
            text:data.message,
